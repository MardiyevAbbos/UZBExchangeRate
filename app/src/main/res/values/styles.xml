<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Button_Green">
        <item name="android:textColor">@color/button_text_color_selector</item>
        <item name="backgroundTint">@color/button_green_selector</item>
    </style>

    <style name="MyWeekDayTextAppearance" parent="TextAppearance.AppCompat.Caption">
        <item name="android:textColor">@color/s_green_100</item>
    </style>

    <style name="M16">
        <item name="android:fontFamily">@font/gilroy_medium</item>
        <item name="android:textSize">16sp</item>
        <item name="lineHeight">18sp</item>
        <item name="android:lineSpacingExtra">0sp</item>
    </style>

    <style name="M14">
        <item name="android:fontFamily">@font/gilroy_medium</item>
        <item name="android:textSize">14sp</item>
        <item name="lineHeight">16sp</item>
        <item name="android:lineSpacingExtra">0sp</item>
    </style>

    <style name="R12">
        <item name="android:fontFamily">@font/gilroy_regular</item>
        <item name="android:textSize">12sp</item>
        <item name="lineHeight">14sp</item>
        <item name="android:lineSpacingExtra">0sp</item>
    </style>

    <style name="mediumStyle">
        <item name="fontFamily">@font/gilroy_medium</item>
        <item name="android:textColor">@color/d_black_100</item>
    </style>

    <style name="regularStyle">
        <item name="fontFamily">@font/gilroy_regular</item>
        <item name="android:textColor">@color/d_black_100</item>
    </style>

</resources>